{
  "name": "paper-dog-dash",
  "version": "0.1.0",
  "homepage": "https://gregwoodsfrontier.github.io/gamedevjs2023/",
  "scripts": {
    "dev": "vite",
    "build": "vite build && phaser-asset-pack-hashing -j -r dist --noEmitOnError",
    "preview": "vite preview",
    "editor": "phasereditor2d-launcher -project . -port 1959",
    "editor-remote": "phasereditor2d-launcher -project . -disable-open-browser -public -port 1959",
    "deploy": "git add dist -f && git commit -m \"Adding dist\" && git subtree push --prefix dist origin gh-pages"
  },
  "devDependencies": {
    "@types/node": "^20.1.0",
    "gh-pages": "^5.0.0",
    "phaser-asset-pack-hashing": "^1.0.5",
    "phaser3-rex-plugins": "^1.60.0",
    "phasereditor2d-launcher": "^3.60.3",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "phaser": "^3.60.0"
  }
}
